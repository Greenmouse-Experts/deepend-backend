  
        <!DOCTYPE html>
        <html lang="en">
            <head>
                <meta charset="UTF-8">
                <meta http-equiv="X-UA-Compatible" content="IE=edge">
                <meta name="viewport" content="width=device-width, initial-scale=1.0">
                <link rel="icon" href="assets5/images/deep.png">
                <title>Deepend-Admin Login</title>
                <link rel="stylesheet" href="assets5/css/style.css">
                <link rel="stylesheet" href="assets5/css/bootstrap.min.css">
                <!--Scroll In Animation-->
                <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
                <script src="https://kit.fontawesome.com/997b229808.js" crossorigin="anonymous"></script>
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
            </head>
            <body id="back">
                <section class="logIn">
                    <div class="container">
                        <%- include("../partials/alerts") %>
                        <div class="row">
                            <div class="col-lg-3"></div>
                            <div class="col-lg-6">
                                <div class="sign">
                                    <form class="sign-div" method="post">
                                        <a href="#">
                                            <img src="assets5/images/Group 1.png" draggable="false" alt="">
                                        </a>
                                        <h1>Admin Login</h1>
                                        <div class="line-rule"></div>
                                        <div class="row">
                                            <!--Username-->
                                            <div class="col-lg-12">
                                                <label>Eamil</label>
                                                <div class="row">
                                                    <div class="col-md-12 mb-3">
                                                        <i class="bi bi-envelope"></i>
                                                        <input required type="email" placeholder="Enter Your Email Address" name="email" class="input" required>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--Username Ends-->
                                            <!--Password-->
                                            <div class="col-lg-12">
                                                <label>Password</label>
                                                <div class="row">
                                                    <div class="col-md-12 mb-3">
                                                        <i class="bi bi-file-earmark-lock"></i>
                                                        <input required type="password" placeholder="Enter your Password" name="password" class="input" required>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--Password-->
                                            <div class="col-md-12 mb-3">
                                                <button value="submit" name="submit" type="submit"> LogIn</button>
                                            </div>
                                        </div>
                                    </form>
                                    
                                </div>
                            </div>
                            <div class="col-lg-3"></div>
                        </div>
                    </div>
                </section>
                <!--Scroll In Animation-->
                <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
                <script src="assets5/js/bootstrap.min.js"></script>
                <script src="assets5/js/main.js"></script>
            </body>
        </html>